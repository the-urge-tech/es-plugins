{
  "ltr.list_feature": {
    "stability": "stable",
    "url": {
      "paths": [
        {
          "path": "/_ltr/{store}/_feature",
          "parts": {
            "store": {
              "required": false,
              "type": "string",
              "description": "The store name"
            }
          },
          "methods": [
            "GET"
          ]
        },
        {
          "path": "/_ltr/_feature",
          "methods": [
            "GET"
          ]
        }
      ]
    },
    "body": null,
    "params": {
      "prefix": {
        "required": false,
        "type": "string",
        "description": "Filter elements by name prefix"
      },
      "from": {
        "required": false,
        "type": "integer",
        "description": "Page results"
      },
      "size": {
        "required": false,
        "type": "integer",
        "description": "Page size"
      }
    }
  }
}
